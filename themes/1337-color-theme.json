{
    "name": "Goodvibe",
    "type": "dark",
    "colors": {
        "editor.background": "#1B1B1B",
        "editor.foreground": "#00BCD4",
        "editor.lineHighlightBackground": "#313131",
        "editorWhitespace.foreground": "#424242",
        "editorIndentGuide.background": "#616161",
        "editor.findMatchHighlightBackground": "#DD2C00"
    },

    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#81C784"
            }
        },
        {
            "name": "BinaryOps",
            "scope": [
				"keyword.operator.arithmetic.flint",
				"keyword.operator.logical.flint",
				"keyword.operator.assignment.flint"
            ],
            "settings": {
                "foreground": "#c781b0"
            }
        },
        {
            "name": "Keywords, Punctuation",
            "scope": [
                "keyword.control",
                "storage.type",
                "storage.modifier",
                "punctuation.definition.parameters",
                "punctuation.section.parens.begin",
                "variable.parameter",
                "variable.other.property",
                "support.variable.property.dom",
                "variable.other.object.property",
                "punctuation.separator",
                "punctuation.accessor",
                "punctuation.definition.array"
            ],
            "settings": {
                "foreground": "#00BCD4"
            }
        },
        {
            "name": "Objects",
            "scope": [
                "variable.other.object",
                "support.class.console",
                "support.variable.dom",
                "support.constant",
                "entity.name.type.module",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#80DEEA"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant.numeric",
                "constant.language",
                "support.type.primitive"
            ],
            "settings": {
                "foreground": "#F9A825"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "constant.other.symbol"
            ],
            "settings": {
                "foreground": "#FBC02D"
            }
        },
        {
            "name": "Quotes",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end"
            ],
            "settings": {
                "foreground": "#00BCD4"
            }
        },
        {
            "name": "Method definitions",
            "scope": [
                "entity.name.function",
                "support.function.dom",
                "support.function"
            ],
            "settings": {
                "foreground": "#ECEFF1",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable.language",
                "variable.other.member",
                "variable.other.readwrite",
                "punctuation.definition.variable"
            ],
            "settings": {
                "foreground": "#80DEEA"
            }
        }
    ]
}